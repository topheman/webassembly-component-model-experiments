interface transport {
  record plugin-response {
    color: option<string>,
    status: repl-status,
    output: option<string>,
  }

  enum repl-status {
    success,
    error,
    warning,
  }

  record readline-response {
    command: string,
    payload: string,
  }

  record plugin-config {
    command: string,
    arg-count: option<s8>,
    man: string,
  }

  record repl-var {
    key: string,
    value: string,
  }
}
